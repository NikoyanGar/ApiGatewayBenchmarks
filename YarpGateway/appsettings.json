{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "backend": {
        "ClusterId": "backendCluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "backendCluster": {
        "Destinations": {
          "d1": {
            "Address": "http://backend-api:8080/"
          }
        }
      }
    }
  }
}
